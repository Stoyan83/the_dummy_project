<% if current_user %>
  <% if !current_user.guest %>
    <h1> Welcome <%= current_user.username.capitalize %></h1>  
  <% elsif current_user.guest %>
    <h1> Welcome <%= current_user.username[0..4].capitalize %></h1>
    <h1> Welcome <%= current_user.id %></h1> 
  <% end %>
<% else %>
    <h1> Your free account has expired please Sign Up</h1>
    <h1> Welcome <%= current_or_guest_user.username[0..4].capitalize %></h1>  
    <h1> Welcome <%= current_or_guest_user.guest? %></h1>  
    <h1> Welcome <%= current_or_guest_user.id %></h1> 
    <h1> Welcome <%= guest_user.id %></h1> 
<% end %>



